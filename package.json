{
  "name": "dev-manager",
  "version": "0.1.0",
  "private": true,
  "description": "Dev Manager - Electron Application",
  "main": "electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:vite dev:electron",
    "dev:vite": "vite",
    "dev:electron": "NODE_ENV=development electron .",
    "build": "vite build && electron-builder",
    "build:dir": "vite build && electron-builder --dir"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.3",
    "vue-router": "^4.5.0"
  }
}